<%- partial('_partial/head') %>
<body>


  <% if(theme.toolbox && theme.toolbox.isShow == true && page.base !== '') { %>
    <span id="toolbox-mobile" class="toolbox-mobile"><%=  theme.toolbox.text %></span>
  <% } %>

  <%- body %>

  <%- partial('_partial/component/modal') %>


  <% if (theme.post.showComments && page.layout === 'post'){ %>
    <%- partial('_partial/component/comments', {className: 'comments-post'}) %>
  <% } else if (page.comments) { %>
    <%- partial('_partial/component/comments', {className: 'comments-' + page.layout}) %>
  <% } %>

  <%- partial('_partial/load-script') %>

  <% if(theme.canvas_nest) { %>
    <script type="text/javascript"
     color="255,0,0" opacity='0.7' zIndex="-2" count="40" src="//cdn.bootcss.com/canvas-nest.js/1.0.1/canvas-nest.js"></script>
    <script type="text/javascript"
     color="0,255,0" opacity='0.7' zIndex="-2" count="40" src="//cdn.bootcss.com/canvas-nest.js/1.0.1/canvas-nest.js"></script>
    <script type="text/javascript"
     color="0,0,255" opacity='0.7' zIndex="-2" count="40" src="//cdn.bootcss.com/canvas-nest.js/1.0.1/canvas-nest.js"></script>
  <% } %>
</body>
</html>
